<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_pressed="false">
        <inset android:insetBottom="5dp" android:insetLeft="5dp" android:insetRight="5dp" android:insetTop="5dp">
            <shape android:shape="rectangle">
                <corners android:radius="6dp"/>
                <solid android:color="#94325580"/>
                <stroke android:width="0dp" android:color="#94325580"/>
                <padding android:bottom="16dp" android:left="16dp" android:right="16dp" android:top="16dp"/>
            </shape>
        </inset>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <inset android:insetBottom="5dp" android:insetLeft="5dp" android:insetRight="5dp" android:insetTop="5dp">
            <shape android:shape="rectangle">
                <corners android:radius="6dp"/>
                <solid android:color="#325580"/>
                <stroke android:width="0dp" android:color="#325580"/>
                <padding android:bottom="16dp" android:left="16dp" android:right="16dp" android:top="16dp"/>
            </shape>
        </inset>
    </item>

</selector>